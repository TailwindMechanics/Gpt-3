.codeBlockContainer {
    padding: 4px 8px;
}

.codeBlock {
    white-space: normal;
    unity-font: resource('LegacyRuntime.ttf');
    font-size: 11px;
    background-color: rgb(56, 56, 56);
    color: rgb(204, 204, 204);
    padding: 4px 8px;
    border-left-color: rgb(100, 100, 100);
    border-right-color: rgb(100, 100, 100);
    border-top-color: rgb(100, 100, 100);
